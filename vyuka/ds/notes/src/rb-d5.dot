digraph G {
	margin = 0;
	ordering = out;
	packmode = "array_ut2"; // 2 beside each other, in User order, Top-aligned

	x1 [ label="x (?=c)" ];
	w1 [ label="w (b)" ];
	s1 [ label="s (b)" ];
	y1 [ label="y (r)" ];
	t1 [ label="t (b)" ];
	u1 [ label="u (?=a)" ];
	v1 [ label="v (b)" ];

	x2 [ label="x (c)" ];
	w2 [ label="w (b)" ];
	s2 [ label="s (b)" ];
	y2 [ label="y (a)" ];
	t2 [ label="t (b)" ];
	u2 [ label="u (b)" ];
	v2 [ label="v (b)" ];


	u1 -> { w1; v1 };
	w1 -> { x1; y1 };
	y1 -> { s1; t1 };

	y2 -> { w2; u2 };
	w2 -> { x2; s2 };
	u2 -> { t2; v2 };

}

