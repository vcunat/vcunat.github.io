digraph G {
	margin = 0;
	ordering = out;

	t1 [ label="t (b)" ];
	w1 [ label="w (b)" ];
	u1 [ label="u (r)" ];
	s1 [ label="s (b)" ];
	v1 [ label="v (r)" ];

	t2 [ label="t (r)" ];
