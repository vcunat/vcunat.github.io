digraph G {
	margin = 0;
	ordering = out;

	node [ shape = triangle ];

	w -> { A, v };
	v -> { u, D };
	u -> { B, C };


	A2 [ label=A ];
	B2 [ label=B ];
	C2 [ label=C ];
	D2 [ label=D ];
	u2 [ label=u ];
	v2 [ label=v ];
	w2 [ label=w ];
	
	u2 -> { v2; w2 };
	v2 -> { A2; B2 };
	w2 -> { C2; D2 };


	u, v, w    [ shape = ellipse ];
	u2, v2, w2 [ shape = ellipse ];
}

